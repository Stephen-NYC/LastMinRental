    <h2 class="h3 mb-3 font-weight-normal">Welcome to LastMinRental</h1>
      
      <div class="form-label-group">
        <%= form.label :username, class:"sr-only" %>
        <%= form.text_field :username, type: 'text', class:"form-control", placeholder:"Username", :required => true, autofocus: true  %>
      </div>
      <div class="form-label-group">
        <%= form.label :first_name, class:"sr-only" %>
        <%= form.text_field :first_name, type: 'text', class:"form-control", placeholder:"First Name", :required => true, autofocus: true  %>
      </div>

      <div class="text-field">
        <%= form.label :last_name, class:"sr-only" %>
        <%= form.text_field :last_name, type: 'text', class:"form-control", placeholder:"Last Name", :required => true %>
      </div>

      <div class="text-field">
        <%= form.label :email, class:"sr-only" %>
        <%= form.text_field :email, type: 'email', class:"form-control", placeholder:"Email Address", :required => true %>
      </div>

      <div class="password-field">
        <%= form.label :password, class:"sr-only" %>
        <%= form.password_field :password, class:"form-control", placeholder:"Password", :required => true %>
      </div>

      <div>
    <%= form.select :location, options_for_select([

  ["Queens", "Queens"],
  ["Brooklyn", "Brooklyn"],
  ["Manhattan", "Manhattan"],
  ["Bronx", "Bronx"],
  ["Staten Island", "Staten Island"]
].map {|times| [times[0],times[1]]}, :selected => params[:user]), {}, { :class => 'span3 controls controls-row', placeholder:"Borough" } %>
      </div>
